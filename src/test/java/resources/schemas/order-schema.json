{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "format": "int64"
    },
    "petId": {
      "type": "integer",
      "format": "int64"
    },
    "quantity": {
      "type": "integer",
      "format": "int32"
    },
    "shipDate": {
      "type": "string",
      "format": "date-time"
    },
    "status": {
      "type": "string",
      "enum": ["placed", "approved", "delivered"]
    },
    "complete": {
      "type": "boolean"
    }
  },
  "required": ["id", "petId", "quantity", "status", "complete"],
  "additionalProperties": false
}
